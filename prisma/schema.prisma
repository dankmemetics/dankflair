generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider        = "mongodb"
  url             = env("DATABASE_URL")
}

model DankFlair {
  id              Int @id @map("_id")
  owner           String
}

model DankFusion {
  id              Int @id @map("_id")
  owner           String
  name            String
  description     String
  dankId          Int
  fusionUrl       String
  fusionContract  String
  fusionId        Int
  fusionX         String
  fusionY         String
  fusionClip      Int
}